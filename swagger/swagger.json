{
    "swagger": "2.0",
    "info": {
        "description": "API for managing irrigation analytics within an agricultural platform.",
        "title": "Irrigation Analytics API",
        "contact": {},
        "version": "0.0.1"
    },
    "basePath": "/",
    "paths": {
        "/health": {
            "get": {
                "description": "Returns the service health status and version",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "health"
                ],
                "summary": "Health check",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/model.HealthResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/v1/farms/{farm_id}/irrigation/analytics": {
            "get": {
                "description": "Returns comprehensive irrigation analytics with year-over-year comparison, time-series data, and sector breakdown",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "analytics"
                ],
                "summary": "Get irrigation analytics for a farm",
                "parameters": [
                    {
                        "type": "integer",
                        "example": 1,
                        "description": "Farm ID",
                        "name": "farm_id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "example": "2024-01-01",
                        "description": "Start date (YYYY-MM-DD format, defaults to 90 days ago)",
                        "name": "start_date",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "example": "2024-01-31",
                        "description": "End date (YYYY-MM-DD format, defaults to today)",
                        "name": "end_date",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "example": 5,
                        "description": "Filter by specific irrigation sector (optional)",
                        "name": "sector_id",
                        "in": "query"
                    },
                    {
                        "enum": [
                            "daily",
                            "weekly",
                            "monthly"
                        ],
                        "type": "string",
                        "example": "daily",
                        "description": "Aggregation granularity: daily, weekly, monthly (default: daily)",
                        "name": "aggregation",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "example": 1,
                        "description": "Page number for time-series results (1-indexed, default: 1)",
                        "name": "page",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "example": 50,
                        "description": "Results per page (default: 50, max: 1000, use 'all' for all results)",
                        "name": "limit",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Analytics data with complete year-over-year comparison",
                        "schema": {
                            "$ref": "#/definitions/model.IrrigationAnalyticsResponse"
                        }
                    },
                    "206": {
                        "description": "Partial content - previous year data incomplete or missing",
                        "schema": {
                            "$ref": "#/definitions/model.IrrigationAnalyticsResponse"
                        }
                    },
                    "400": {
                        "description": "Invalid request parameters or date format",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "404": {
                        "description": "Farm not found",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "model.AnalyticsMetrics": {
            "type": "object",
            "properties": {
                "average_efficiency": {
                    "type": "number",
                    "example": 0.85
                },
                "efficiency_range": {
                    "$ref": "#/definitions/model.EfficiencyRange"
                },
                "total_irrigation_events": {
                    "type": "integer",
                    "example": 120
                },
                "total_irrigation_volume_mm": {
                    "type": "number",
                    "example": 450.5
                }
            }
        },
        "model.EfficiencyRange": {
            "type": "object",
            "properties": {
                "max": {
                    "type": "number",
                    "example": 0.98
                },
                "min": {
                    "type": "number",
                    "example": 0.72
                }
            }
        },
        "model.HealthResponse": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string"
                },
                "status": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                }
            }
        },
        "model.IrrigationAnalyticsPeriod": {
            "type": "object",
            "properties": {
                "end": {
                    "type": "string",
                    "example": "2024-01-31T23:59:59Z"
                },
                "start": {
                    "type": "string",
                    "example": "2024-01-01T00:00:00Z"
                }
            }
        },
        "model.IrrigationAnalyticsResponse": {
            "type": "object",
            "properties": {
                "aggregation": {
                    "type": "string",
                    "example": "daily"
                },
                "farm_id": {
                    "type": "integer",
                    "example": 1
                },
                "farm_name": {
                    "type": "string",
                    "example": "Green Valley Farm"
                },
                "metrics": {
                    "$ref": "#/definitions/model.AnalyticsMetrics"
                },
                "period": {
                    "$ref": "#/definitions/model.IrrigationAnalyticsPeriod"
                },
                "period_comparison": {
                    "$ref": "#/definitions/model.PeriodComparisonSet"
                },
                "same_period_-1": {
                    "$ref": "#/definitions/model.YoYComparison"
                },
                "same_period_-2": {
                    "$ref": "#/definitions/model.YoYComparison"
                },
                "sector_breakdown": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/model.SectorBreakdown"
                    }
                },
                "time_series": {
                    "$ref": "#/definitions/model.TimeSeries"
                }
            }
        },
        "model.PaginationMetadata": {
            "type": "object",
            "properties": {
                "limit": {
                    "type": "integer",
                    "example": 50
                },
                "page": {
                    "type": "integer",
                    "example": 1
                },
                "total_count": {
                    "type": "integer",
                    "example": 250
                },
                "total_pages": {
                    "type": "integer",
                    "example": 5
                }
            }
        },
        "model.PeriodComparison": {
            "type": "object",
            "properties": {
                "efficiency_change_percent": {
                    "type": "number",
                    "example": 3.7
                },
                "events_change_percent": {
                    "type": "number",
                    "example": 4.3
                },
                "volume_change_percent": {
                    "type": "number",
                    "example": 7.2
                }
            }
        },
        "model.PeriodComparisonSet": {
            "type": "object",
            "properties": {
                "vs_same_period_-1": {
                    "$ref": "#/definitions/model.PeriodComparison"
                },
                "vs_same_period_-2": {
                    "$ref": "#/definitions/model.PeriodComparison"
                }
            }
        },
        "model.SectorBreakdown": {
            "type": "object",
            "properties": {
                "average_efficiency": {
                    "type": "number",
                    "example": 0.88
                },
                "sector_id": {
                    "type": "integer",
                    "example": 1
                },
                "sector_name": {
                    "type": "string",
                    "example": "North Field"
                },
                "total_volume_mm": {
                    "type": "number",
                    "example": 150.2
                }
            }
        },
        "model.TimeSeries": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/model.TimeSeriesEntry"
                    }
                },
                "pagination": {
                    "$ref": "#/definitions/model.PaginationMetadata"
                }
            }
        },
        "model.TimeSeriesEntry": {
            "type": "object",
            "properties": {
                "date": {
                    "type": "string",
                    "example": "2024-01-01"
                },
                "efficiency": {
                    "type": "number",
                    "example": 0.864
                },
                "event_count": {
                    "type": "integer",
                    "example": 3
                },
                "nominal_amount_mm": {
                    "type": "number",
                    "example": 12.5
                },
                "real_amount_mm": {
                    "type": "number",
                    "example": 10.8
                }
            }
        },
        "model.YoYComparison": {
            "type": "object",
            "properties": {
                "average_efficiency": {
                    "type": "number"
                },
                "data_incomplete": {
                    "type": "boolean"
                },
                "efficiency_range": {
                    "$ref": "#/definitions/model.EfficiencyRange"
                },
                "note": {
                    "type": "string"
                },
                "total_irrigation_events": {
                    "type": "integer"
                },
                "total_irrigation_volume_mm": {
                    "type": "number"
                }
            }
        }
    }
}